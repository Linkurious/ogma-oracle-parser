{
  "compilerOptions": {
    "target": "ES5",
    "emitDeclarationOnly": false,
    "declaration": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
